                                        <ns0:height unit="cm">24.2</ns0:height>
                                        <ns0:width unit="cm">34</ns0:width>
                                        <ns0:depth unit="cm">30.8</ns0:depth>
                                    </ns0:dimensions>
                                </ns0:support>
                                <ns0:condition />
		    </ns0:supportDesc>
                            <ns0:layoutDesc>
                                <ns0:layout><ns0:p>Two lines of Greek letters on the side of the chest</ns0:p>
                                    <ns0:rs ana="#execution.chiselled" ref="http://www.eagle-network.eu/voc/writing/lod/1">chiselled</ns0:rs>
                                	<ns0:damage />
			</ns0:layout>
                            </ns0:layoutDesc>
                        </ns0:objectDesc>
                        <ns0:handDesc>
                            <ns0:handNote><ns0:p>Tall regular letters, deeply cut: Alpha with broken bar; omicron in rhomboid form; sigma lunate but square in form; kappa with short arms; epsilon standard with equal length horizontal bars; a large diamond shape interpunct in line 1</ns0:p>
                                <ns0:locus from="line1" to="line1">Line 1</ns0:locus>
                                <ns0:dimensions type="letterHeight">
                                    <ns0:height unit="mm" atLeast="13" atMost="13">50-60 (but omicron sigma 40-45)</ns0:height>
                                </ns0:dimensions>
                                <ns0:locus from="line2" to="line2">Line 2</ns0:locus>
                                <ns0:dimensions type="letterHeight">
                                    <ns0:height unit="mm" atLeast="62" atMost="66">62-66</ns0:height>
                                </ns0:dimensions>
                                <ns0:locus from="line1" to="line2">Interlineation line 1 to 2</ns0:locus>
                                <ns0:dimensions type="interlinear">
                                    <ns0:height unit="mm" atLeast="62" atMost="66">not measured</ns0:height>
                                </ns0:dimensions>
                            </ns0:handNote>
