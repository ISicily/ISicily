                    <ns0:physDesc>
                        <ns0:objectDesc>
                            <ns0:supportDesc>
                                <ns0:support><ns0:p>A square funerary chest of the local brown limestone</ns0:p>
                                    <ns0:material ana="#material.stone.limestone" ref="http://www.eagle-network.eu/voc/material/lod/66.html">limestone</ns0:material>
                                    <ns0:objectType ana="#object.ash_chest" ref="http://www.eagle-network.eu/voc/objtyp/lod/79.html">ash chest</ns0:objectType>
