                    <lb n="1" xml:id="ASoAK"/><persName type="attested" xml:id="ASoAU"><name><w n="5" xml:id="ASoAe">Ianuarius</w></name></persName> <g ref="#interpunct" xml:id="ASoAo" n="10">·</g>
                    <lb n="2" xml:id="ASoAy"/><w xml:id="ASoAΙ" n="15"><expan xml:id="ASoAΤ"><abbr xml:id="ASoAε">ser</abbr><ex xml:id="ASoAο">vus</ex></expan></w> <g ref="#interpunct" xml:id="ASoBA" n="20">·</g> <w xml:id="ASoBK" n="25"><expan xml:id="ASoBU"><abbr xml:id="ASoBe">vix</abbr><ex xml:id="ASoBo">it</ex></expan></w> <g ref="#interpunct" xml:id="ASoBy" n="30">·</g> <w xml:id="ASoBΙ" n="35"><expan xml:id="ASoBΤ"><abbr xml:id="ASoBε">an</abbr><ex xml:id="ASoBο">nis</ex></expan></w> <g ref="#interpunct" xml:id="ASoCA" n="40">·</g> <num value="18"><w n="45" xml:id="ASoCK">XIIX</w></num>
