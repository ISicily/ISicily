            <ns0:div type="edition" xml:space="preserve" xml:lang="la" resp="#JP">
                <ns0:ab>
                    <ns0:lb n="1" /><ns0:gap reason="lost" extent="unknown" unit="character" />&#183; Iulio &#183; <ns0:gap reason="illegible" unit="character" quantity="1" /><ns0:gap reason="lost" extent="unknown" unit="character" />
                    <ns0:lb n="2" /><ns0:gap reason="lost" extent="unknown" unit="character" />Divi &#183; A<ns0:supplied reason="lost">ugusti</ns0:supplied><ns0:gap reason="lost" extent="unknown" unit="character" />
                    <ns0:lb /><ns0:gap reason="lost" extent="unknown" unit="line" />
                </ns0:ab>
