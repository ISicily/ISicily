                <ab>
                    <lb n="1"/>Μ(άρκον) Ἰούν(ιον) Φήλι<supplied reason="lost">κα</supplied>
                    <lb n="2"/><roleName type="civic" subtype="bouletes">βουλευτ(ὴν)</roleName> Λιλυ<supplied reason="lost">βα</supplied>
			<lb n="3" break="no"/>ειτῶν φιλοτει<supplied reason="lost">μη</supplied>
                    <lb n="4" break="no"/>σάμενον περἰ τὴ<supplied reason="lost">ν</supplied>
                    <lb n="5"/>πατρίδα τὸ κοινὸ<supplied reason="lost">ν</supplied>
                    <lb n="6"/>Κινακων ἐκ τῶν
			<lb n="7"/>ἰδίων <roleName type="paracivic" subtype="patronus">πάτρω<supplied reason="lost">νι</supplied></roleName>
                    <lb n="8"/>ἀξίῳ
			<lb n="9"/>δ δ β
                </ab>
