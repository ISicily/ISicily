            <ns0:div type="edition" xml:space="preserve" xml:lang="grc" resp="#martaf">
                <ns0:ab>
                    <ns0:lb n="1" /><ns0:persName type="divine"><ns0:name><ns0:supplied reason="lost">&#934;</ns0:supplied><ns0:supplied reason="undefined" evidence="previouseditor">&#959;&#8150;&#946;&#959;&#957;</ns0:supplied></ns0:name> <ns0:w>&#7936;&#954;&#949;&#961;&#963;&#949;&#954;&#972;&#956;&#951;&#957;</ns0:w>
                    <ns0:lb n="2" /><ns0:w><ns0:supplied reason="lost">&#7953;&#954;</ns0:supplied>&#945;&#964;&#951;&#946;&#972;&#955;&#959;&#957;</ns0:w> <ns0:name>&#7944;&#960;&#972;&#955;&#955;&#969;&#957;<ns0:unclear>&#945;</ns0:unclear></ns0:name></ns0:persName>
                    <ns0:lb n="3" /><ns0:w><ns0:supplied reason="lost">&#7985;</ns0:supplied>&#948;&#961;&#965;&#963;&#940;&#956;&#951;&#957;</ns0:w> <ns0:w>&#963;&#951;<ns0:unclear>&#954;</ns0:unclear><ns0:supplied reason="lost">&#8183;</ns0:supplied></ns0:w> <ns0:w><ns0:supplied reason="lost">&#964;&#8217;</ns0:supplied></ns0:w> <ns0:w><ns0:supplied reason="lost">&#7952;&#960;&#8054;</ns0:supplied></ns0:w>
                    <ns0:lb n="4" /><ns0:w>&#954;&#972;&#963;&#956;&#959;&#957;</ns0:w> <ns0:w><ns0:unclear>&#954;</ns0:unclear><ns0:supplied reason="lost" cert="low">&#945;&#955;&#8056;&#957;</ns0:supplied></ns0:w> <ns0:w><ns0:supplied reason="lost" cert="low">&#7956;&#952;&#951;&#954;&#945;</ns0:supplied></ns0:w><ns0:space quantity="1" unit="line" />
                    <ns0:lb n="5" /><ns0:persName type="attested"><ns0:name><ns0:seg part="I">&#928;&#945;&#964;&#961;<ns0:gap reason="illegible" quantity="1" unit="character" /></ns0:seg><ns0:gap reason="lost" extent="unknown" unit="character" /></ns0:name></ns0:persName>
                </ns0:ab>
