                    <lb n="4"/><persName type="attested"><name type="idionym"><supplied reason="lost">Ἀ</supplied>ριστο̄́<supplied reason="omitted">ι</supplied></name> 
                    <lb n="5"/><persName type="attested"><name type="patronym"><seg type="I">Ματρ</seg><gap reason="lost" quantity="3" unit="character"/></name></persName></persName>
                    <lb n="6"/><persName type="attested"><name type="idionym"><gap reason="lost" extent="unknown" unit="character"/><seg part="F">ιας</seg></name></persName>
                </ab>
