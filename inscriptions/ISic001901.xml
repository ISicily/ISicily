                <ab>
                    <lb n="1" style="text-direction:r-to-l"/><persName><name><w part="I"><expan><abbr>Q</abbr><ex>uint</ex></expan></w></name> <g ref="#interpunct">·</g> <name><w part="I"><expan><abbr><hi rend="ligature">Ant</hi>o</abbr><ex>ni</ex></expan></w></name> <g ref="#interpunct">·</g> <name><w part="I"><expan><abbr><hi rend="ligature">Aq</hi></abbr><ex>u</ex></expan></w></name></persName>
