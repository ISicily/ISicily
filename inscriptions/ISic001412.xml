                    <div type="edition" xml:space="preserve" xml:lang="grc">
                        <ab>
                            <lb n="1"/>Ἐπὶ <roleName type="civic.religious" subtype="hieromnamon">ἱερομνάμονος</roleName> Σα<supplied reason="lost">ν</supplied>νίω<supplied reason="lost">νος</supplied> <supplied reason="lost">τοῦ</supplied> <supplied reason="lost">δεῖνα</supplied>
                            <lb n="2"/><roleName type="civic" subtype="agonothetes">ἀγωνοθετᾶν</roleName> Κλεάνδρου τοῦ Αρ<gap reason="lost" unit="character" extent="unknown"/>
                            <lb n="3"/>Παρμ<gap reason="lost" unit="character" extent="unknown"/><supplied reason="lost">τοῦ</supplied><gap reason="lost" unit="character" extent="unknown"/>
                            <lb n="4"/>Ζωίλου τοῦ Θέστ<supplied reason="lost" cert="low">ωνος</supplied> <gap reason="lost" unit="character" extent="unknown"/>
                            <lb n="5"/>λαμπαδιστ<supplied reason="lost">αὶ</supplied><gap reason="lost" unit="character" extent="unknown"/>
                         </ab>
