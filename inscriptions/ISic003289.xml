                    <ns0:lb n="2" /><ns0:gap reason="lost" extent="unknown" unit="character" /> <ns0:w><ns0:expan><ns0:abbr>&#7952;&#964;</ns0:abbr><ns0:ex cert="low">&#8182;&#957;</ns0:ex></ns0:expan></ns0:w> <ns0:num value="40">&#956;</ns0:num> <ns0:w>&#965;&#7985;&#8056;&#962;</ns0:w> <ns0:persName type="attested"><ns0:name><ns0:expan><ns0:abbr>&#7992;&#963;&#953;&#948;&#974;&#961;&#945;</ns0:abbr><ns0:ex>&#962;</ns0:ex></ns0:expan></ns0:name></ns0:persName> <ns0:gap reason="illegible" quantity="1" unit="character" /><ns0:gap reason="lost" extent="unknown" unit="character" />
                    <ns0:lb n="3" /><ns0:gap reason="lost" extent="unknown" unit="character" /> <ns0:w><ns0:supplied reason="lost">&#954;&#945;&#964;</ns0:supplied><ns0:unclear>&#949;</ns0:unclear>&#964;&#941;&#952;&#949;&#953;</ns0:w> <ns0:w>&#948;&#8050;</ns0:w> <ns0:w>&#954;&#945;&#8054;</ns0:w> <ns0:persName><ns0:name><ns0:w part="I">&#7992;&#963;&#953;&#948;&#969;<ns0:supplied reason="lost">&#961;</ns0:supplied></ns0:w> <ns0:gap reason="lost" extent="unknown" unit="character" /></ns0:name></ns0:persName>
                    <ns0:lb n="4" /><ns0:gap reason="lost" extent="unknown" unit="character" /><ns0:unclear>&#933;</ns0:unclear>&#932;<ns0:hi rend="superscript">&#919;</ns0:hi> <ns0:w><ns0:expan><ns0:abbr>&#964;</ns0:abbr><ns0:ex>&#8182;&#957;</ns0:ex></ns0:expan></ns0:w> <ns0:placeName type="ethnic"><ns0:expan><ns0:abbr>&#922;&#945;&#964;&#945;&#957;</ns0:abbr><ns0:ex>&#8182;&#957;</ns0:ex></ns0:expan></ns0:placeName> &#928;<ns0:hi rend="superscript">&#927;</ns0:hi>&#918;&#919;<ns0:unclear>&#923;</ns0:unclear><ns0:gap reason="lost" extent="unknown" unit="character" />
