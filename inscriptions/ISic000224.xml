            <div type="edition" xml:space="preserve" xml:lang="la" resp="#VM #AA">
                <ab>
                    <lb n="1"/><w n="5">Dis</w> <w n="10"><expan><abbr>man</abbr><ex>ibus</ex></expan></w>
                    <lb n="2"/><persName type="attested"><name><w n="15"><expan><abbr>L</abbr><ex>ucius</ex></expan></w></name> <name><w n="20">Tennius</w></name>
