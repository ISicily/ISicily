                <ab>
                    <w n="5" lemma="dis">Dis</w>
                    <w n="10" lemma="manes">manibus</w>
                    <w n="15" lemma="Lucius">Lucius</w>
                    <w n="20" lemma="Tennius">Tennius</w>
                    <w n="25" lemma="Herma">Herma</w>
