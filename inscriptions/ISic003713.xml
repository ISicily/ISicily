                                        <ns0:width unit="cm">12</ns0:width>
                                        <ns0:depth unit="cm">10</ns0:depth>
                                    </ns0:dimensions>
                                </ns0:support>
                                <ns0:condition ana="#condition.fragment" />
		    </ns0:supportDesc>
                            <ns0:layoutDesc>
                                <ns0:layout><ns0:p>Three letters filling the sloping face of the rim</ns0:p>
                                    <ns0:rs ana="#execution.chiselled" ref="http://www.eagle-network.eu/voc/writing/lod/1">chiselled</ns0:rs>
