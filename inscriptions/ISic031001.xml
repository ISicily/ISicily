                           <dimensions><!-- from Bettarini -->
                              <height unit="cm">3.7</height>
                              <width unit="cm">8</width>
                              <depth unit="cm"/>
                           </dimensions>
                        </support>
                        <condition ana="#condition.complete.broken"/>
                     </supportDesc>
                     <layoutDesc>
                        <layout>
                           <p>Greek text inscribed on both sides, beginning on the recto in 9 lines, boustrophedon (first line l-to-r), and continuing on the verso in a further two lines, also boustrophedon (first line l-to-r).</p>
                           <rs ana="#execution.engraved" ref="https://www.eagle-network.eu/voc/writing/lod/3.html">engraved</rs>
                           <damage ana="#text_condition.complete #text_condition.deteriorated"/>
                           </layout>
                     </layoutDesc>
                  </objectDesc>
