                  <objectDesc>
                     <supportDesc>
                        <support>
                           <p>A rectangular lead sheet in three joining pieces, originally folded five times. Heavily corroded.</p>
                           <material ana="#material.metal.lead" ref="http://www.eagle-network.eu/voc/material/lod/108.html">Lead</material>
                           <objectType ana="#object.lamina" ref="http://www.eagle-network.eu/voc/objtyp/lod/145.html">lamina</objectType>
                           <dimensions>
                              <height unit="cm">3.7</height>
                              <width unit="cm">8</width>
                              <depth unit="cm" />
                           </dimensions>
                        </support>
                        <condition ana="#condition.complete.broken" />
                     </supportDesc>
                     <layoutDesc>
                        <layout>
                           <p>Greek text inscribed on both sides, beginning on the recto in 9 lines, boustrophedon (first line l-to-r), and continuing on the verso in a further two lines, also boustrophedon (first line l-to-r).</p>
                           <rs ana="#execution.engraved" ref="https://www.eagle-network.eu/voc/writing/lod/3.html">engraved</rs>
                           <damage ana="#text_condition.complete #text_condition.deteriorated" />
                           </layout>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc>
                     <handNote><p>presence of qoppa</p>
                         <locus from="line1" to="line11">Line 1-11</locus>
                        <dimensions type="letterHeight">
                           <height unit="mm" atLeast="3" atMost="3">3</height>
                        </dimensions> 
                        <locus from="line1" to="line2">Interlineation</locus>
                        <dimensions type="interlinear">
                           <height unit="mm" />
                        </dimensions>
                     </handNote>
                  </handDesc>
