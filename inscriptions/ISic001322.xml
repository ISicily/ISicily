                    <ns0:lb n="4" /><ns0:g ref="#interpunct">&#183;</ns0:g> <ns0:num value="32">&#955;&#946;</ns0:num> <ns0:g ref="#interpunct">&#183;</ns0:g> <ns0:persName type="attested"><ns0:name>&#931;&#945;&#946;&#959;&#973;&#954;&#953;&#962;</ns0:name> <ns0:g ref="#interpunct">&#183;</ns0:g>
                    <ns0:lb n="5" /><ns0:name>&#918;&#969;&#943;&#955;<ns0:hi rend="ligature">&#959;&#962;</ns0:hi></ns0:name></ns0:persName> <ns0:g ref="#interpunct">&#183;</ns0:g> <ns0:w>&#960;&#940;&#964;<ns0:hi rend="ligature">&#961;&#969;&#957;</ns0:hi></ns0:w> <ns0:g ref="#interpunct">&#183;</ns0:g>
                    <ns0:lb n="6" /><ns0:w>&#7952;&#960;&#959;&#943;&#951;&#963;&#949;</ns0:w> <ns0:g ref="#interpunct">&#183;</ns0:g>
