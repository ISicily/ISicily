            <ns0:div type="edition" xml:space="preserve" xml:lang="grc" resp="#martaf">
                <ns0:ab>
                    <ns0:lb n="1" /><ns0:persName type="attested"><ns0:name>&#931;&#945;&#946;&#959;&#8166;&#954;&#953;&#962;</ns0:name>
