                    <lb n="1"/>ἐτελεύτησεν
                    <lb n="2"/><persName type="attested"><name>Παῦλος</name> <placeName type="ethnic">Ἐφέ 
                    <lb n="3" break="no"/>σιος</placeName></persName> ἐτῶν
                    <lb n="4"/><num value="50">πεντήκοντα</num>
                    <lb n="5"/>τῇ πρό <num value="3">γ</num> <expan><abbr>καλ</abbr><ex>ανδῶν</ex></expan>
