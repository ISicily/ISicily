            <div type="edition" xml:space="preserve" xml:lang="la" resp="#JP">
                <ab>
                    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><orig>od</orig><gap reason="illegible" unit="character" quantity="1"/><gap reason="lost" extent="unknown" unit="character"/>
