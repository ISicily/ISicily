            <div type="edition" xml:lang="grc" resp="#JP">
                <div type="textpart" subtype="fragment" n="1" xml:id="fr1">
                <ab>
                    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/>ΕΡΩ<gap reason="lost" extent="unknown" unit="character"/>
                </ab>
            </div>
                <div type="textpart" subtype="fragment" n="2" xml:id="fr2">
