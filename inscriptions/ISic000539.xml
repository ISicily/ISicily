                    <lb n="1"/><persName type="attested"><name><expan><abbr>L</abbr><ex>ucius</ex></expan></name> <g ref="#interpunct">·</g> <name>Seius</name> <g ref="#interpunct">·</g> <persName type="attested"><name type="patronym"><expan><abbr>L</abbr><ex>uci</ex></expan></name></persName> <g ref="#interpunct">·</g> <expan><abbr>f</abbr><ex>ilius</ex></expan> <g ref="#interpunct">·</g> <expan><abbr>Aem</abbr><ex>ila</ex></expan> <supplied reason="subaudible">tribu</supplied> <name><w part="I">Fir</w></name><gap reason="lost" extent="unknown" unit="character"/></persName>
