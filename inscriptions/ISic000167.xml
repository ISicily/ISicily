        <body>
            <div type="edition" xml:space="preserve" xml:lang="la" resp="#TA #AA">
                <ab>
                    <lb n="1" xml:id="AQΤAK"/><persName type="attested" xml:id="AQΤAU"><name><w n="5" xml:id="AQΤAe">Daphne</w></name></persName>
                    <lb n="2" xml:id="AQΤAo"/><w xml:id="AQΤAy" n="10"><expan xml:id="AQΤAΙ"><abbr xml:id="AQΤAΤ">vix</abbr><ex xml:id="AQΤAε">it</ex></expan></w> <g ref="#interpunct" xml:id="AQΤAο" n="15">·</g> <w xml:id="AQΤBA" n="20"><expan xml:id="AQΤBK"><abbr xml:id="AQΤBU">ann</abbr><ex xml:id="AQΤBe">is</ex></expan></w> <g ref="#interpunct" xml:id="AQΤBo" n="25">·</g> <num value="23"><w n="30" xml:id="AQΤBy">XXIII</w></num>
                    <lb n="3" xml:id="AQΤBΙ"/><persName type="attested" xml:id="AQΤBΤ"><name><w n="35" xml:id="AQΤBε">Canin<hi rend="tall" xml:id="AQΤBο">i</hi></w></name> <g ref="#interpunct" xml:id="AQΤCA" n="40">·</g> <name><w n="45" xml:id="AQΤCK">Agonis</w></name></persName>
                </ab>
            </div>
            <div type="edition" subtype="simple-lemmatized" resp="#VF">
                <ab>
                    <w n="5" lemma="Daphne">Daphne</w>
                    <w n="10" lemma="vivo">vixit</w>
                    <w n="20" lemma="annus">annis</w>
                    <w n="30" lemma="XXIII">XXIII</w>
                    <w n="35" lemma="Caninius">Canini</w>
