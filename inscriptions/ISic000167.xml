                    <ns0:lb n="2" xml:id="AQ&#932;Ao" /><ns0:w xml:id="AQ&#932;Ay" n="10"><ns0:expan xml:id="AQ&#932;A&#921;"><ns0:abbr xml:id="AQ&#932;A&#932;">vix</ns0:abbr><ns0:ex xml:id="AQ&#932;A&#949;">it</ns0:ex></ns0:expan></ns0:w> <ns0:g ref="#interpunct" xml:id="AQ&#932;A&#959;" n="15">&#183;</ns0:g> <ns0:w xml:id="AQ&#932;BA" n="20"><ns0:expan xml:id="AQ&#932;BK"><ns0:abbr xml:id="AQ&#932;BU">ann</ns0:abbr><ns0:ex xml:id="AQ&#932;Be">is</ns0:ex></ns0:expan></ns0:w> <ns0:g ref="#interpunct" xml:id="AQ&#932;Bo" n="25">&#183;</ns0:g> <ns0:num value="23"><ns0:w n="30" xml:id="AQ&#932;By">XXIII</ns0:w></ns0:num>
