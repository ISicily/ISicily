        <body>
            <div type="edition" xml:space="preserve">
                <div type="textpart" subtype="fragment-physical" n="a" xml:lang="xly-Grek"> 
                    <ab>
                        <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/>ΠΛΛΛΛ<gap reason="lost" extent="unknown" unit="character"/>
                        <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/><unclear>Ε</unclear><gap reason="lost" extent="unknown" unit="character"/>
                    </ab>
               </div>
            
               <div type="textpart" subtype="fragment-physical" n="b" xml:lang="xly-Grek"> 
                   <ab>
                       <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/>Δ
                       <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/>Α
                       <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>
                   </ab>
               </div>
