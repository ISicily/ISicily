            <div type="edition" xml:space="preserve" xml:lang="la" resp="#TA">
                <ab>
                    <lb n="1"/><expan><abbr>Cn</abbr><ex>aeo</ex></expan> <g ref="#interpunct">·</g> Pollieno <g ref="#interpunct">·</g> <expan><abbr>Cn</abbr><ex>aei</ex></expan> <expan><abbr>f</abbr><ex>ilio</ex></expan>
                    <lb n="2"/><roleName type="military" subtype="tribunus.militum"><expan><abbr>tr</abbr><ex>ibuno</ex></expan> <g ref="#interpunct">·</g> <expan><abbr>mil</abbr><ex>itum</ex></expan></roleName> <g ref="#interpunct">·</g>
                    <lb n="3"/><expan><abbr>legio</abbr><ex>nis</ex></expan> <g ref="#interpunct">·</g> <num value="12">XII</num>
