            <div type="edition" xml:space="preserve" xml:lang="la" resp="#TA #AA">
                <ab>
                    <lb n="1" xml:id="ANKAK"/><persName type="attested" xml:id="ANKAU"><name><w n="5" xml:id="ANKAe"><expan xml:id="ANKAo"><abbr xml:id="ANKAy">M</abbr><ex xml:id="ANKAΙ">arcus</ex></expan></w></name> <g ref="#interpunct" xml:id="ANKAΤ" n="10">·</g> <name><w n="15" xml:id="ANKAε">Antonius</w></name>
                    <lb n="2" xml:id="ANKAο"/><name><w n="20" xml:id="ANKBA">Natalis</w></name></persName> <g ref="#interpunct" xml:id="ANKBK" n="25">·</g> <w xml:id="ANKBU" n="30"><expan xml:id="ANKBe"><abbr xml:id="ANKBo">vix</abbr><ex xml:id="ANKBy">it</ex></expan></w>
                    <lb n="3" xml:id="ANKBΙ"/><w xml:id="ANKBΤ" n="35"><expan xml:id="ANKBε"><abbr xml:id="ANKBο">an</abbr><ex xml:id="ANKCA">nis</ex></expan></w> <g ref="#interpunct" xml:id="ANKCK" n="40">·</g> <num value="12"><w n="45" xml:id="ANKCU">XII</w></num>
                </ab>
            </div>
            <div type="edition" subtype="simple-lemmatized" resp="#RC">
                <ab>
                    <w n="5" lemma="Marcus">Marcus</w>
                    <w n="15" lemma="Antonius">Antonius</w>
                    <w n="20" lemma="Natalis">Natalis</w>
                    <w n="30" lemma="vivo">vixit</w>
