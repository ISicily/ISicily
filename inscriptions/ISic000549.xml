                <ab>
                    <lb n="1"/><name>Ulpio</name> <g ref="#interpunct">·</g> <name>Severo</name>
                    <lb n="2"/><name>Nar<supplied reason="lost">is</supplied>tus</name>
                    <lb n="3"/><roleName type="military" subtype="eques.singularis"><expan><abbr>eq</abbr><ex>ues</ex></expan> <g ref="#interpunct">·</g> <expan><abbr>sing</abbr><ex>ularis</ex></expan></roleName> <g ref="#interpunct">·</g> <expan><abbr>Aug</abbr><ex>usti</ex></expan> <g ref="#interpunct">·</g> <expan><abbr>tur</abbr><ex>ma</ex></expan>
                    <lb n="4"/>Busturonis <g ref="#interpunct">·</g> <expan><abbr>vix</abbr><ex>it</ex></expan> <g ref="#interpunct">·</g> <expan><abbr>an</abbr><ex>nos</ex></expan> <g ref="#interpunct">·</g> <num value="38">XXXVIII</num> <g ref="#interpunct">·</g> D<gap reason="lost" extent="unknown" unit="character"/>
                    <lb n="5"/><name>Mansuetus</name> <g ref="#interpunct">·</g> et <g ref="#interpunct">·</g> <name>Titus</name>
                    <lb n="6"/><expan><abbr>h</abbr><ex>eredes</ex></expan> <g ref="#interpunct">·</g> <expan><abbr>b</abbr><ex>ene</ex></expan> <g ref="#interpunct">·</g> <expan><abbr>m</abbr><ex>erenti</ex></expan> <g ref="#interpunct">·</g> <expan><abbr>f</abbr><ex>ecerunt</ex></expan>
                </ab>
