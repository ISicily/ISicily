                <ab>
                    <lb n="1"/>Dis <g type="interpunct">·</g> Manibus <g type="interpunct">·</g> <expan><abbr>sac</abbr><ex>rum</ex></expan>
                    <lb n="2"/>Callis<choice><corr>t</corr><sic>i</sic></choice>o <g type="interpunct">·</g> <choice><corr>t</corr><sic>i</sic></choice>raeci <g type="interpunct">·</g> <expan><abbr>Treb</abbr><ex cert="low">oni</ex></expan> <g type="interpunct">·</g>
