                    <lb n="2" xml:id="BKΙAε"/><w xml:id="BKΙAο">τὰν</w> <space unit="character" quantity="1" xml:id="BKΙBA"/><w xml:id="BKΙBK">ἀδελφὰν</w> <w xml:id="BKΙBU">αὐτοῦτα</w> 
