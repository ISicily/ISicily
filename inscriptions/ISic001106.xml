                    <lb n="4" xml:id="BKΙCA"/><persName type="divine" xml:id="BKΙCK"><name xml:id="BKΙCU">Ἀφροδίται</name> <space unit="character" quantity="1" xml:id="BKΙCe"/><name type="epithet" xml:id="BKΙCo">Οὐρανίαι</name></persName>
                </ab>
