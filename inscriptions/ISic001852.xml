        <ns0:body>
            <ns0:div type="edition" xml:space="preserve" xml:lang="grc" resp="#JP" source="#autopsy">
                <ns0:ab>
                    <ns0:lb n="1" /><ns0:persName type="attested"><ns0:name><ns0:gap reason="lost" unit="character" extent="unknown" /><ns0:w part="F">&#953;&#945;&#957;&#949;</ns0:w></ns0:name></ns0:persName>
                    <ns0:lb n="2" /><ns0:supplied reason="lost">&#967;&#961;&#951;</ns0:supplied><ns0:unclear>&#963;</ns0:unclear>&#964;&#8051; &#967;&#945;&#8150;
                    <ns0:lb n="3" break="no" /><ns0:supplied reason="lost">&#961;&#949;</ns0:supplied> <ns0:supplied reason="lost">&#7956;&#950;</ns0:supplied>&#951;&#963;&#945;&#962;
