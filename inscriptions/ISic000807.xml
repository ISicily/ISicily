                <ab>
                    <lb n="1"/>Σέξτου
                    <lb n="2"/>Πομπηίου
                    <lb n="3"/>Ἄπρου
                    <lb n="4"/>Παλάκου
                    <lb n="5"/>Πομπείας
                    <lb n="6"/><gap reason="lost" unit="character" quantity="1"/><unclear>ο</unclear><gap reason="lost" unit="character" quantity="2"/><unclear>τ</unclear>ου
