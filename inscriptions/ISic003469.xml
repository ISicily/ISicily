                    <lb n="6"/><name><expan><abbr>M</abbr><ex>arci</ex></expan></name> <g ref="#interpunct">·</g> <name>Livi</name> <g ref="#interpunct">·</g> <name><expan><abbr>M</abbr><ex>arci</ex></expan></name> <g ref="#interpunct">·</g> <expan><abbr>f</abbr><ex>ili</ex></expan> <g ref="#interpunct">·</g> <expan><abbr>Qui</abbr><ex>rina</ex></expan> <g ref="#interpunct">·</g> <name>Optati</name> <g ref="#interpunct">·</g> flaminis <g ref="#interpunct">·</g> <expan><abbr>G<supplied reason="lost">a</supplied>ul</abbr><ex>itanorum</ex></expan> <g ref="#interpunct">·</g>
                    <lb n="7"/>Iuliae <g ref="#interpunct">·</g> Augusti <add place="overstrike"><expan><abbr>Imp</abbr><ex>eratoris</ex></expan> <g ref="#interpunct">·</g> <expan><abbr>perpet</abbr><ex>ui</ex></expan></add> <g ref="#interpunct">·</g> cum <orig>V</orig> <!-- the V here is suggested in some versions, but see EAGLE photos for various reconstructions? -->
                    <lb n="8"/>liberis <g ref="#interpunct">·</g> <expan><abbr>s</abbr><ex>ua</ex></expan> <g ref="#interpunct">·</g> <expan><abbr>p</abbr><ex>ecunia</ex></expan> consacravit
