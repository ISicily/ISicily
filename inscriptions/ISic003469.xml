                <ab>
                    <lb n="1"/>Cereri <g ref="#interpunct">·</g> <name>Iuliae</name> <g ref="#interpunct">·</g> <name>Augustae</name>
                    <lb n="2"/>Divi <g ref="#interpunct">·</g> Augusti <g ref="#interpunct">·</g> Matri
                    <lb n="3"/><name><expan><abbr>Ti</abbr><ex>beri</ex></expan></name> <g ref="#interpunct">·</g> Caesaris <g ref="#interpunct">·</g> Augusti
