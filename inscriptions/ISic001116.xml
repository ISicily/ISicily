            <ns0:div type="edition" xml:space="preserve" xml:lang="grc" resp="#VM #JP">
                <ns0:ab>
                    <ns0:lb n="1" /><ns0:persName type="attested"><ns0:name>&#922;&#955;&#949;&#945;&#947;&#972;&#961;&#945;&#962;</ns0:name> <ns0:placeName type="ethnic">&#924;&#945;&#963;&#963;&#945;&#955;&#953;&#974;&#964;&#951;&#962;</ns0:placeName></ns0:persName> <ns0:persName type="divine">&#7944;&#966;&#961;&#959;&#948;&#943;&#964;&#951;&#953;</ns0:persName>
                    <ns0:lb n="2" /><ns0:unclear>&#7936;</ns0:unclear>&#957;&#941;&#952;&#951;&#954;&#949; &#964;&#961;&#940;&#960;&#949;&#950;&#945;&#957;
                </ns0:ab>
