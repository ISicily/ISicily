            <ns0:div type="apparatus" resp="#JP">
                <ns0:listApp>
                    <ns0:app><ns0:note>Text of IGPalermo checked against photograph</ns0:note></ns0:app>
                    <ns0:app loc="2"><ns0:note>Manni Piraino: &#964;&#961;&#940;&#960;&#951;&#958;&#945;&#957; (typographic error?)</ns0:note></ns0:app>
                </ns0:listApp>
