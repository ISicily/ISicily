        <body>
            <div type="edition" subtype="EDR" n="EDR081511" xml:lang="la">
                <ab>
                    <lb n="1"/>
                    <expan>
                        <abbr>D</abbr>
                        <ex>is</ex>
                    </expan>
                    <expan>
                        <abbr>M</abbr>
                        <ex>anibus</ex>
                    </expan>
                    <expan>
                        <abbr>s</abbr>
