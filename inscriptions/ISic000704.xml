                    <ns0:lb n="3" break="no" /><ns0:g ref="#interpunct">&#183;</ns0:g>us</ns0:w></ns0:name></ns0:persName> <ns0:g ref="#interpunct">&#183;</ns0:g> vixit <ns0:g ref="#interpunct">&#183;</ns0:g> <ns0:expan><ns0:abbr>an</ns0:abbr><ns0:ex>nos</ns0:ex></ns0:expan> <ns0:g ref="#interpunct">&#183;</ns0:g> <ns0:num value="40">XXXX</ns0:num> <ns0:g ref="#interpunct">&#183;</ns0:g>
                    <ns0:lb n="4" /><ns0:g ref="#interpunct">&#183;</ns0:g> uxor <ns0:g ref="#interpunct">&#183;</ns0:g> <ns0:expan><ns0:abbr>b</ns0:abbr><ns0:ex>ene</ns0:ex></ns0:expan> <ns0:g ref="#interpunct">&#183;</ns0:g> <ns0:expan><ns0:abbr>mer</ns0:abbr><ns0:ex>enti</ns0:ex></ns0:expan> <ns0:g ref="#interpunct">&#183;</ns0:g> <ns0:expan><ns0:abbr>fec</ns0:abbr><ns0:ex>it</ns0:ex></ns0:expan> <ns0:g ref="#interpunct">&#183;</ns0:g>
                </ns0:ab>
