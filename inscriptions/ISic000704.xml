            <ns0:div type="edition" xml:space="preserve" xml:lang="la" resp="#JP">
                <ns0:ab>
                    <ns0:lb n="1" /><ns0:expan><ns0:abbr>D</ns0:abbr><ns0:ex>is</ns0:ex></ns0:expan> <ns0:g ref="#interpunct">&#183;</ns0:g> <ns0:expan><ns0:abbr>m</ns0:abbr><ns0:ex>anibus</ns0:ex></ns0:expan> <ns0:g ref="#interpunct">&#183;</ns0:g> <ns0:expan><ns0:abbr>s</ns0:abbr><ns0:ex>acrum</ns0:ex></ns0:expan> <ns0:g ref="#interpunct">&#183;</ns0:g>
