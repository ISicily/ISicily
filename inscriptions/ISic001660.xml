                                    <ns0:height unit="mm" atLeast="25" atMost="25">25</ns0:height>
                                </ns0:dimensions>
                                <ns0:locus from="line1" to="line2">Interlineation line 1 to 2</ns0:locus>
                                <ns0:dimensions type="interlinear">
                                    <ns0:height unit="mm" />
                                </ns0:dimensions>
                            </ns0:handNote>
