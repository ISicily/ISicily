                                        <ns0:width unit="cm">70</ns0:width>
                                        <ns0:depth unit="cm">20.5</ns0:depth>
                                    </ns0:dimensions>
                                </ns0:support>
                                <ns0:condition />
		    </ns0:supportDesc>
                            <ns0:layoutDesc>
                                <ns0:layout>
                                    <ns0:rs ana="#execution.chiselled" ref="http://www.eagle-network.eu/voc/writing/lod/1">chiselled</ns0:rs>
                                	<ns0:damage />
			</ns0:layout>
                            </ns0:layoutDesc>
                        </ns0:objectDesc>
                        <ns0:handDesc>
                            <ns0:handNote><ns0:p>Regular and neat letters without serifs. Alpha with both straight and broken bar, beta with closed loops (upper slightly smaller), Mu has full length slanting strokes; omicron full size; sigma has horizontal top and bottom; rhomboid omega.</ns0:p>
                                <ns0:locus from="line1" to="line5">Line 1-5</ns0:locus>
