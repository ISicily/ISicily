                    <lb n="1" xml:id="APKAK"/><persName type="attested" xml:id="APKAU"><name><w n="5" xml:id="APKAe"><expan xml:id="APKAo"><abbr xml:id="APKAy">T</abbr><ex xml:id="APKAΙ">itus</ex></expan></w></name> <g ref="#interpunct" xml:id="APKAΤ" n="10">·</g> <name><w n="15" xml:id="APKAε">Cispius</w></name> <g ref="#interpunct" xml:id="APKAο" n="20">·</g> <persName type="attested" xml:id="APKBA"><name type="libertinatio"><w n="25" xml:id="APKBK"><expan xml:id="APKBU"><abbr xml:id="APKBe">T</abbr><ex xml:id="APKBo">iti</ex></expan></w></name></persName> <g ref="#interpunct" xml:id="APKBy" n="30">·</g> <w xml:id="APKBΙ" n="35"><expan xml:id="APKBΤ"><abbr xml:id="APKBε"><unclear xml:id="APKBο">l</unclear></abbr><ex xml:id="APKCA">ibertus</ex></expan></w>
