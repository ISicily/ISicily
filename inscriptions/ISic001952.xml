                <ab>
                    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/> <persName type="attested"><name type="patronym"><supplied reason="lost">Νυ</supplied>μφοδώρου</name></persName> 
                    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/><persName type="attested"><name type="patronym"><w part="F">οδώρου</w></name></persName>
                    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/><persName type="attested"><name type="patronym"><w part="F">α</w></name></persName>
                    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/><persName type="attested"><name type="patronym"><w part="F">κλείου</w></name></persName>
                    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/><persName type="attested"><persName type="attested"><name type="patronym"><w part="F">ο<unclear>τ</unclear>ίου</w></name></persName> <name type="demotic"><w><abbr>Περ</abbr></w></name></persName>
                    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/><persName type="attested"><name type="patronym"><w part="F">κράτεος</w></name></persName>
                    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/><persName type="attested"><persName type="attested"><name type="patronym"><w part="F">ώρου</w></name></persName> <name type="demotic"><w><abbr>Σαλ</abbr></w></name></persName>
                    <lb n="8"/><gap reason="lost" extent="unknown" unit="character"/> <persName type="attested"><name type="patronym"><unclear>Μ</unclear>άμου</name></persName>
