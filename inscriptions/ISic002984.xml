                   <lb n="3"/>Tettia <g ref="#interpunct">·</g> <expan><abbr><unclear>L</unclear></abbr><ex>uci</ex></expan> <supplied reason="lost"><expan><abbr>f</abbr><ex>ilia</ex></expan></supplied> <gap reason="lost" extent="unknown" unit="character"/>
                   <lb n="4"/><roleName type="civic.religious" subtype="sacerdos">sacerd<supplied reason="lost">os</supplied></roleName> <supplied reason="lost" cert="low"><expan><abbr>publ</abbr><ex>ica</ex></expan></supplied>
                   <lb n="5"/>Cereris <gap reason="lost" unit="character" extent="unknown"/>
                   <lb n="6"/>mater <g ref="#interpunct">·</g> <expan><abbr>l</abbr><ex>ocus</ex></expan> <g ref="#interpunct">·</g> <supplied reason="lost"><expan><abbr>d</abbr><ex>atus</ex></expan></supplied> <supplied reason="lost"><expan><abbr>d</abbr><ex>ecreto</ex></expan></supplied> <supplied reason="lost"><expan><abbr>d</abbr><ex>ecurionum</ex></expan></supplied>
