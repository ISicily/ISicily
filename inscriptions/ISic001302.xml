            <ns0:div type="edition" xml:space="preserve" xml:lang="grc" resp="#martaf #AA">
                <ns0:ab>
                    <ns0:lb n="1" /><ns0:w><ns0:expan><ns0:abbr>&#920;</ns0:abbr><ns0:ex>&#949;&#959;&#8150;&#962;</ns0:ex></ns0:expan></ns0:w> <ns0:g ref="#ivy-leaf">&#10086;</ns0:g> <ns0:w><ns0:expan><ns0:abbr>&#922;</ns0:abbr><ns0:ex>&#945;&#964;&#945;&#954;&#952;&#959;&#957;&#943;&#959;&#953;&#962;</ns0:ex></ns0:expan></ns0:w>
                    <ns0:lb n="2" /><ns0:persName type="attested"><ns0:name>&#7992;&#959;&#965;&#955;&#943;&#945;</ns0:name>
                    <ns0:lb n="3" /><ns0:name>&#915;&#945;&#955;&#942;&#957;&#951;</ns0:name></ns0:persName>
                    <ns0:lb n="4" /><ns0:w>&#7969;</ns0:w> <ns0:w>&#966;&#953;&#955;&#972;&#963;&#964;&#959;&#955;&#959;&#962;</ns0:w>
                    <ns0:lb n="5" /><ns0:w>&#7956;&#950;&#951;&#963;&#949;</ns0:w> <ns0:g ref="#interpunct">&#183;</ns0:g> <ns0:w>&#7956;&#964;&#951;</ns0:w>
                    <ns0:lb n="6" /><ns0:hi rend="supraline"><ns0:num value="33">&#955;&#947;</ns0:num></ns0:hi>
                </ns0:ab>
