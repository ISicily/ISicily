                                </dimensions>
                                <locus from="line3" to="line3">Line 3</locus>
                                <dimensions type="letterHeight">
                                    <height unit="mm">35</height>
                                </dimensions>
                                <locus from="line1" to="line2">Interlineation</locus>
                                <dimensions type="interlinear">
                                    <height unit="mm">not measured</height>
                                </dimensions>
                            </handNote>
                        </handDesc>
