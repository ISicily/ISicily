                <ns0:ab>
                    <ns0:lb n="1" /><ns0:gap reason="lost" extent="unknown" unit="character" /><ns0:supplied reason="undefined" evidence="previouseditor"><ns0:orig>LI</ns0:orig></ns0:supplied><ns0:gap reason="lost" extent="unknown" unit="character" />
                    <ns0:lb n="2" /><ns0:gap reason="lost" extent="unknown" unit="character" /><ns0:gap reason="illegible" unit="character" quantity="1" /><ns0:orig>LATYR</ns0:orig><ns0:gap reason="lost" extent="unknown" unit="character" />
                    <ns0:lb n="3" /><ns0:gap reason="lost" extent="unknown" unit="character" /><ns0:orig>RI<ns0:hi rend="tall">I</ns0:hi>OVES</ns0:orig><ns0:gap reason="lost" extent="unknown" unit="character" />
                    <ns0:lb n="4" /><ns0:gap reason="lost" extent="unknown" unit="character" /><ns0:orig>IOR</ns0:orig> Sicul<ns0:gap reason="lost" extent="unknown" unit="character" />
                    <ns0:lb n="5" /><ns0:gap reason="lost" extent="unknown" unit="character" /> pampine<ns0:unclear>i</ns0:unclear> <ns0:gap reason="lost" extent="unknown" unit="character" />  
                    <ns0:lb n="6" /><ns0:gap reason="lost" extent="unknown" unit="character" /> <ns0:unclear>S</ns0:unclear>irac<ns0:gap reason="lost" extent="unknown" unit="character" /> 
                    <ns0:lb n="6" /><ns0:gap reason="lost" extent="unknown" unit="character" /><ns0:orig>P<ns0:unclear>P</ns0:unclear></ns0:orig><ns0:gap reason="lost" extent="unknown" unit="character" />          
                </ns0:ab>
