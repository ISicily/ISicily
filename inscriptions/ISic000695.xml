                <ab>
                    <lb n="1"/><persName type="attested"><supplied reason="lost" cert="low"><roleName type="supracivic" subtype="imperator"><w><expan><abbr>Imp</abbr><ex>eratori</ex></expan></w></roleName></supplied> <supplied reason="lost" cert="low"><name><expan><abbr>Caes</abbr><ex>ari</ex></expan></name></supplied> <supplied reason="lost" cert="low"><name type="praenomen"><expan><abbr>M</abbr><ex>arco</ex></expan></name></supplied> <name type="nomen"><orig>Ulp</orig><supplied reason="lost" unit="character">io</supplied></name> <supplied reason="lost" cert="low"><name>Traiano</name></supplied></persName> <gap reason="lost" extent="unknown" unit="character"/>
                </ab>
