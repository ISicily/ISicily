            <ns0:div type="edition" xml:space="preserve" xml:lang="grc" resp="#JP">
                <ns0:ab>
                    <ns0:lb n="1" xml:id="Bv&#932;AK" /><ns0:persName type="attested" xml:id="Bv&#932;AU"><ns0:name xml:id="Bv&#932;Ae">&#7944;&#947;&#941;&#956;&#945;&#967;&#959;&#962;</ns0:name> <ns0:space unit="character" quantity="1" xml:id="Bv&#932;Ao" /><ns0:persName type="attested" xml:id="Bv&#932;Ay"><ns0:name type="patronym" xml:id="Bv&#932;A&#921;">&#928;&#959;&#955;&#965;&#940;&#961;&#967;&#959;&#965;</ns0:name></ns0:persName></ns0:persName> 
