                    <ns0:lb n="1" /><ns0:g ref="#alpha">&#913;</ns0:g> <ns0:g ref="#christogram" /> <ns0:g ref="#sigma">&#931;</ns0:g> <ns0:w>&#7960;&#957;&#952;&#940;&#948;&#949;</ns0:w> <ns0:w>&#954;&#8150;<ns0:lb n="2" break="no" />&#964;&#949;</ns0:w> <ns0:persName type="attested"><ns0:name>&#922;&#949;&#954;&#953;&#955;&#953;&#945;<ns0:lb n="3" break="no" />&#957;&#8056;&#962;</ns0:name></ns0:persName> <ns0:w>&#950;&#942;&#963;&#945;&#962;</ns0:w>
                    <ns0:lb n="4" /><ns0:w>&#7956;&#964;&#951;</ns0:w> <ns0:num value="20"><ns0:hi rend="supraline">&#954;</ns0:hi></ns0:num> <ns0:w>&#917;&#7984;&#961;&#942;<ns0:lb n="5" break="no" />v<ns0:choice><ns0:corr>&#951;</ns0:corr><ns0:sic>&#921;&#921;</ns0:sic></ns0:choice></ns0:w> <ns0:w>&#8017;&#956;&#8150;&#957;</ns0:w> <ns0:w>&#960;&#8118;<ns0:lb n="6" break="no" />&#963;&#953;&#957;</ns0:w> <ns0:w>&#7952;&#957;</ns0:w> <ns0:persName type="divine"><ns0:expan><ns0:abbr>&#920;</ns0:abbr><ns0:ex>&#949;</ns0:ex><ns0:abbr>&#8183;</ns0:abbr></ns0:expan></ns0:persName>
                    <ns0:lb n="7" /><ns0:g ref="#alpha">&#913;</ns0:g> <ns0:g ref="#christogram" /> <ns0:g ref="#sigma">&#931;</ns0:g> <ns0:g ref="#figure_zoomorphic" />
