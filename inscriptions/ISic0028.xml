                    <lb break="no" n="4"/>mo <g type="interpunct">·</g> patrono <g type="interpunct">·</g> <expan><abbr>col</abbr><ex>oniae</ex></expan>
                    <lb n="5"/><expan><abbr>Panhormit</abbr><ex>anorum</ex></expan> <g type="interpunct">·</g> princi
                    <lb break="no" n="6"/>pales <g type="interpunct">·</g> viri <g type="interpunct">·</g> ex <g type="interpunct">·</g> aere col
                    <lb break="no" n="7"/>lato <expan><abbr>d</abbr><ex>ono</ex></expan> <g type="interpunct">·</g> <expan><abbr>d</abbr><ex>ederunt</ex></expan>
                </ab>
