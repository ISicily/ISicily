                    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/><unclear>b</unclear><unclear>i</unclear><unclear>l</unclear><unclear>l</unclear><unclear>i</unclear> <orig>m</orig> <orig>r</orig>
                    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/><w><supplied reason="lost">poste</supplied>risque</w> <g ref="#interpunct"/> <w>suis</w>
                    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/><w><unclear>a</unclear></w> <w>moneta</w> <g ref="#interpunct"/>
