            <div type="edition" subtype="simple-lemmatized" resp="#RC">
                <ab>
                    <w n="5" lemma="Auctarius">Auctario</w>
                    <w n="15" lemma="Primigenius">Primigeni</w>
                    <w n="25" lemma="filius">filio</w>
                    <w n="35" lemma="vivo">vixit</w>
                    <w n="45" lemma="annus">annis</w>
