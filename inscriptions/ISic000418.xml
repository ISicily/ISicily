        <body>
            <div type="edition" xml:space="preserve" xml:lang="la" resp="#JP">
                <ab>
                    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><orig><unclear>N</unclear>E<unclear>R</unclear></orig><gap reason="lost" extent="unknown" unit="character"/>
                </ab>
