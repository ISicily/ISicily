                                    </dimensions>
                                </support>
                                <condition/>
		    </supportDesc>
                            <layoutDesc>
                                <layout><p>Three lines of Latin letters, with vacat in line 3.</p>
                                    <rs ana="#execution.chiselled" ref="http://www.eagle-network.eu/voc/writing/lod/1">chiselled</rs>
                                	<damage/>
			</layout>
