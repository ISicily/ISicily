                <ab>
                    <lb n="1"/><persName type="attested" role="sacerdos"><name>Antiae</name> <persName type="attested"><name type="patronym"><expan><abbr>M</abbr><ex>arci</ex></expan></name></persName> <expan><abbr>f</abbr><ex>iliae</ex></expan> <name>Cleo
                    <lb n="2" break="no"/>patrae</name></persName> <roleName type="religious" subtype="sacerdos">sacerdoti</roleName>
                    <lb n="3"/>ex voluntate <expan><abbr>pop</abbr><ex>uli</ex></expan>
                    <lb n="4"/><expan><abbr>d</abbr><ex>ecreto</ex></expan> <expan><abbr>d</abbr><ex>ecurionum</ex></expan> inpensa <expan><abbr>pub</abbr><ex>lico</ex></expan>
                    <lb n="5"/>remissa cuius d<supplied reason="lost">e</supplied>
                    <lb n="6" break="no"/>dicatione ple<supplied reason="lost">beis</supplied>
                    <lb n="7"/>singuli <roleName type="civic" subtype="decurio">decu<supplied reason="lost">ri</supplied>
                    <lb n="8" break="no"/>onum</roleName> filis b<supplied reason="lost">ini</supplied>
                    <lb n="9"/><roleName type="civic" subtype="decurio">decurioni<supplied reason="lost">bus</supplied></roleName>
                    <lb n="10"/>quini den<supplied reason="lost">arii</supplied>                   
                    <lb n="11"/>dati sunt
                </ab>
